datasource eratosthenes {
    provider = "sqlite"
    url      = "file:../../eratosthenes.sqlite?query_timeout=21000"
}

generator eratosthenes_client {
    provider = "prisma-client"
    output   = "../../lib/eratosthenes_prisma"
    runtime  = "deno"
}

model numbers {
    id      Int     @id
    isprime Boolean
}
